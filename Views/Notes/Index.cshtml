@model List<Note>

<h2>Lista de notas</h2>
<hr />
<p>
	<a asp-action="Create">Crear</a>
</p>
<table>
	<thead>
		<tr>
			<td>Titulo</td>
			<td>Contenido</td>
		</tr>
	</thead>
	<tbody>
		@foreach(var nota in Model)
		{
			<tr>
				<td>@nota.Title</td>
				<td>@nota.Content</td>
			</tr>
		}
	</tbody>
</table>
